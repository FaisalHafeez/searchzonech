{
  "name": "app-search-reference-ui-react",
  "version": "1.2.0",
  "license": "Apache-2.0",
  "private": true,
  "dependencies": {
    "@elastic/react-search-ui": "^1.4.1",
    "@elastic/search-ui-app-search-connector": "^1.4.1",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "history": "^5.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-router-dom": "^6.0.0-beta.0",
    "react-scripts": "^3.4.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start": "react-scripts start",
    "start-dev": "npm-run-all -p watch-css start",
    "build": "npm-run-all build-css",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "npm-run-all build-css"
  },
  "devDependencies": {
    "enzyme": "^3.4.4",
    "enzyme-to-json": "^3.3.4",
    "node-sass-chokidar": "^1.4.0",
    "npm-run-all": "^4.1.3",
    "rewire": "^4.0.1"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "browserslist": [
    ">0.2%",
    "chrome 44",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
